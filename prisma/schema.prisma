generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:root@localhost:8889/Qbase"
}

model User {
  id          Int      @id @default(autoincrement())
  username    String
  surname     String
  phonenumber String    @unique
  password    String
  cardnumber  String   @default("0000 0000 0000 0000")
  createdAt   DateTime @default(now())
}
